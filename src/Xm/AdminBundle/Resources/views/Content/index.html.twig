{% extends "XmAdminBundle:Layout:base.html.twig" %}
{% block javascripts %}
    {{ parent() }}
    {% javascripts
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
{% block stylesheets %}
{% stylesheets filter='cssrewrite' output='css/*.css'
%}
    <link rel="stylesheet" href="{{ asset_url }}" />
{% endstylesheets %}
{% endblock %}
{% block content_header %}
    <h1>
        页面管理
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="{{ path('dashboard') }}"><i class="fa fa-dashboard"></i> 首页</a></li>
        <li class="active">页面管理</li>
    </ol>
{% endblock %}
{% block content %}
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <div class="row show-grid">
        <div class="col-md-12">
            <center><button type="submit" class="btn btn-success btn-sm">保存</button></center>
        </div>
    </div>
    {{ form_end(form) }}
{% endblock %}