<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}网站管理后台{% endblock %}</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    {% javascripts
    '@XmAdminBundle/Resources/public/plugins/jQuery/jQuery-2.2.0.min.js'
    '@XmAdminBundle/Resources/public/bootstrap/js/bootstrap.min.js'
    '@XmAdminBundle/Resources/public/plugins/iCheck/icheck.min.js'
    'https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js'
    'https://oss.maxcdn.com/respond/1.4.2/respond.min.js'

    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% stylesheets filter='cssrewrite'
    'bundles/xmadmin/bootstrap/css/bootstrap.min.css'
    'bundles/xmadmin/dist/css/AdminLTE.min.css'
    'bundles/xmadmin/plugins/iCheck/square/blue.css'
    'bundles/xmadmin/dist/css/ionicons.min.css'
    'bundles/xmadmin/dist/css/font-awesome.min.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {% block javascripts %}
    {% endblock %}
    {% block stylesheets %}
    {% endblock %}
</head>
<body class="hold-transition login-page">
{% block content %}
{% endblock %}
</body>
</html>
